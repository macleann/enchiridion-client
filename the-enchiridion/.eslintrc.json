{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react-hooks/exhaustive-deps": "off",
        "react/react-in-jsx-scope": "off",
        "no-prototype-builtins": "off",
        "no-unused-vars": "off",
        "react/prop-types": "off",
        "react/no-unescaped-entities": "off"
    }
}